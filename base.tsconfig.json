{
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "es5",
    "declaration": true,
    "skipLibCheck": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "preserveSymlinks": true,
    "moduleResolution": "node",
    "noEmit": false,
    "lib": ["es2018"],
    "sourceMap": true,
    "strictFunctionTypes": true
  }
}
