{
  "extends": "./tsconfig.base.json",
  "files": [],
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2017",
    "plugins": [
      {
        "transform": "./packages/tracing-plugin/src/index.ts",
        "moduleMap": {
          "^packages/system/src/(.*)": "$1 (@effect-ts/system)",
          "^packages/system/test/(.*)": "$1 (@effect-ts/system/test)",
          "^packages/core/src/(.*)": "$1 (@effect-ts/core)",
          "^packages/core/test/(.*)": "$1 (@effect-ts/core/test)",
          "^packages/jest/src/(.*)": "$1 (@effect-ts/jest)",
          "^packages/jest/test/(.*)": "$1 (@effect-ts/jest/test)",
          "^packages/monocle/src/(.*)": "$1 (@effect-ts/monocle)",
          "^packages/monocle/test/(.*)": "$1 (@effect-ts/monocle/test)",
          "^packages/morphic/src/(.*)": "$1 (@effect-ts/morphic)",
          "^packages/morphic/test/(.*)": "$1 (@effect-ts/morphic/test)"
        }
      }
    ]
  }
}
