{
  "extends": "./tsconfig.base.json",
  "files": [],
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2017",
    "plugins": [
      {
        "transform": "./packages/tracing-plugin/src/index.ts",
        "moduleMap": {
          "^packages/system/src/(.*)": "(@effect-ts/system): $1",
          "^packages/system/test/(.*)": "(@effect-ts/system/test): $1",
          "^packages/core/src/(.*)": "(@effect-ts/core): $1",
          "^packages/core/test/(.*)": "(@effect-ts/core/test): $1",
          "^packages/jest/src/(.*)": "(@effect-ts/jest): $1",
          "^packages/jest/test/(.*)": "(@effect-ts/jest/test): $1",
          "^packages/monocle/src/(.*)": "(@effect-ts/monocle): $1",
          "^packages/monocle/test/(.*)": "(@effect-ts/monocle/test): $1",
          "^packages/morphic/src/(.*)": "(@effect-ts/morphic): $1",
          "^packages/morphic/test/(.*)": "(@effect-ts/morphic/test): $1"
        }
      }
    ]
  }
}
