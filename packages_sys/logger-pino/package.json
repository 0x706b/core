{
  "name": "@matechs/logger-pino",
  "version": "2.0.2",
  "license": "MIT",
  "private": false,
  "repository": "https://github.com/mikearnaldi/matechs-effect.git",
  "scripts": {
    "build": "rimraf lib && rimraf es6 && tsc --build tsconfig.build.json && tsc --build tsconfig.es6.json && yarn import-path-rewrite",
    "postbuild": "yarn prettier --write \"./{lib,es6}/**/*.ts\"",
    "lint": "eslint . --ext .ts,.tsx",
    "autofix": "yarn prettier && yarn lint --fix && yarn prettier",
    "prettier": "prettier --write \"./{src,tests,demo}/**/*.ts\"",
    "import-path-rewrite": "import-path-rewrite",
    "tc": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "module": "es6/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib",
    "es6"
  ],
  "dependencies": {
    "@matechs/logger": "^5.0.2",
    "@matechs/prelude": "^2.0.2",
    "@types/pino": "^6.0.0",
    "pino": "^6.2.0"
  },
  "devDependencies": {
    "@matechs/test-jest": "^2.0.2"
  }
}
