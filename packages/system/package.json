{
  "name": "@effect-ts/system",
  "version": "0.37.14",
  "license": "MIT",
  "private": false,
  "repository": "https://github.com/Effect-TS/core.git",
  "homepage": "https://www.matechs.com",
  "scripts": {
    "clean": "rimraf build tsbuildinfo dist",
    "build-cjs": "ttsc -p tsconfig.build.cjs.json",
    "build-esm": "ttsc -p tsconfig.build.esm.json",
    "build-post": "ts-node ../../scripts/pack.ts",
    "build": "concurrently \"yarn build-cjs\" \"yarn build-esm\" && yarn build-post",
    "lint": "eslint . --ext .ts,.tsx",
    "autofix": "yarn prettier && yarn lint --fix && yarn prettier",
    "prettier": "prettier --write \"./{src,test,demo}/**/*.ts\"",
    "tc": "tsc --noEmit",
    "circular": "madge --circular --ts-config ./tsconfig.json --extensions ts ./src"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": [
    "./build/cjs/Operator/index.js",
    "./build/esm/Operator/index.js"
  ],
  "exports": {
    ".": {
      "module": "./build/esm/index.js",
      "require": "./build/cjs/index.js"
    },
    "./*": {
      "module": "./build/esm/*",
      "require": "./build/cjs/*"
    }
  }
}
