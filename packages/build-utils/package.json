{
  "name": "@effect-ts/build-utils",
  "version": "0.8.2",
  "license": "MIT",
  "private": false,
  "repository": "https://github.com/Effect-TS/core.git",
  "homepage": "https://www.matechs.com",
  "scripts": {
    "nodex": "ts-node --compiler ttypescript --project ../../tsconfig.jest.json",
    "clean": "rimraf build tsbuildinfo",
    "build:lib": "ttsc --build  tsconfig.build.json && yarn nodex src/package.ts",
    "build": "yarn run build:lib && yarn run prettier:lib",
    "lint": "eslint . --ext .ts,.tsx",
    "autofix": "yarn prettier && yarn lint --fix && yarn prettier",
    "prettier": "prettier --write \"./{src,test,demo}/**/*.ts\"",
    "prettier:lib": "prettier --write \"./build/**/*.ts\"",
    "tc": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "sideEffects": false,
  "config": {
    "modules": []
  },
  "peerDependencies": {
    "chalk": "^4.1.0",
    "cpx": "^1.5.0",
    "@effect-ts/core": "^0.28.9"
  },
  "devDependencies": {
    "@effect-ts/core": "^0.28.9"
  }
}
